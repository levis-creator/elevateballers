{
  "name": "elevateballers",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "dev:ngrok": "node scripts/ngrok-tunnel.js",
    "dev:tunnel": "npm run dev & npm run dev:ngrok",
    "build": "prisma generate && astro build",
    "build:cpanel": "cross-env DEPLOY_TARGET=cpanel prisma generate && cross-env DEPLOY_TARGET=cpanel astro build",
    "build:vercel": "cross-env DEPLOY_TARGET=vercel prisma generate && cross-env DEPLOY_TARGET=vercel astro build",
    "postinstall": "prisma generate",
    "start": "node server.js",
    "preview": "echo The @astrojs/vercel adapter does not support 'astro preview'. Use 'npm run preview:vercel' instead (requires Vercel CLI) or 'npm run dev' for development.",
    "preview:vercel": "vercel dev",
    "preview:prod": "vercel env pull .env.production && vercel dev",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:cpanel": "npm run build:cpanel",
    "astro": "astro",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "create-admin": "tsx scripts/create-admin.ts",
    "db:migrate-to-production": "tsx scripts/migrate-db-to-production.ts"
  },
  "dependencies": {
